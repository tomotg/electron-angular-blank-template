{
  "name": "electron-angular-blank-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf dist/main-process/**/*",
    "tsc-mainprocess-debug": "npm run clean && npx tsc --project tsconfig-mainprocess.json",
    "tsc-mainprocess-prod": "npm run clean && npx tsc --project tsconfig-mainprocess-prod.json",
    "build-debug": "npm run tsc-mainprocess-debug && ng build",
    "build-prod": "npm run tsc-mainprocess-prod && ng build --prod",
    "build-win": "npm run build-prod && electron-builder --win --x64",
    "build-mac": "npm run build-prod && electron-builder --mac --x64",
    "package-win": "npm run build-prod && electron-packager . VbAsinListCreator --platform=win32 --arch=x64 --overwrite --out=packages --no-prune --ignore=/.vscode --ignore=/build --ignore=/packages --ignore=/e2e --ignore=/patch",
    "package-mac": "npm run build-prod && electron-packager . VbAsinListCreator --platform=darwin --arch=x64 --overwrite --out=packages --no-prune --ignore=/.vscode --ignore=/build --ignore=/packages --ignore=/e2e --ignore=/patch",
    "package-mac-cer": "npm run build-prod && electron-packager . VbAsinListCreator --platform=mas --arch=x64 --overwrite --out=packages --no-prune --ignore=/.vscode --ignore=/build --ignore=/packages --ignore=/e2e --ignore=/patch --app-bundle-id=valuebooks.desktop --extend-info='./plist/team.plist' --osx-sign --app-category-type=public.app-category.social-networking",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "electron": "^2.0.8",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
